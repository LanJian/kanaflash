!!!
html
  head
    title #{title} - Kana Flash

    script(src="/javascripts/html5.js")
    - if (app.NODE_ENV == 'production')
      script(src="https://ajax.googleapis.com/ajax/libs/jquery/1.6.1/jquery.min.js")
      script(src="https://ajax.googleapis.com/ajax/libs/jqueryui/1.8.13/jquery-ui.min.js")
    - else
      script(src="/javascripts/jquery-1.6.1.js")
      script(src="/javascripts/jquery-ui-1.8.13.js")

    link(rel='stylesheet', href='/stylesheets/reset.css')
    link(rel='stylesheet', href='/stylesheets/jquery-ui-1.8.13.css')
    link(rel='stylesheet', href='/stylesheets/all.css')
  body
    #top_banner
    #main
      header
        != partial('header', {})
      #content
        != body
      footer
